{
    "name": "gatsby-site-template",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "start": "gatsby develop",
        "build": "gatsby build",
        "test": "jest test",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "lint": "eslint . --fix"
    },
    "dependencies": {
        "gatsby": "5.15.0",
        "gatsby-plugin-manifest": "5.15.0",
        "gatsby-plugin-react-helmet": "6.15.0",
        "gatsby-plugin-robots-txt": "1.8.0",
        "gatsby-plugin-sharp": "5.15.0",
        "gatsby-plugin-sitemap": "6.15.0",
        "gatsby-plugin-styled-components": "6.15.0",
        "gatsby-remark-images": "7.15.0",
        "gatsby-remark-relative-images": "2.0.5",
        "gatsby-source-filesystem": "5.15.0",
        "gatsby-transformer-remark": "6.15.0",
        "gatsby-transformer-sharp": "5.15.0",
        "prop-types": "15.8.1",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "react-helmet": "6.1.0",
        "styled-components": "5.3.11"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.17.0",
        "@testing-library/react": "14.3.1",
        "@testing-library/react-hooks": "8.0.1",
        "babel-jest": "30.0.5",
        "babel-preset-gatsby": "3.15.0",
        "eslint": "8.57.1",
        "eslint-config-airbnb": "19.0.4",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.1",
        "jest": "30.0.5",
        "jest-axe": "10.0.0",
        "jest-environment-jsdom": "30.0.5",
        "jest-styled-components": "7.2.0",
        "jest-watch-typeahead": "3.0.1"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "coverageReporters": [
            "lcov",
            "text"
        ],
        "moduleNameMapper": {
            "^gatsby-core-utils/(.*)$": "gatsby-core-utils/$1",
            "^gatsby-page-utils/(.*)$": "gatsby-page-utils/$1",
            "^gatsby-plugin-utils/(.*)$": [
                "gatsby-plugin-utils/dist/$1",
                "gatsby-plugin-utils/$1"
            ]
        },
        "setupFiles": [
            "<rootDir>/jest-loader.js"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/utils/test-matchers.js",
            "<rootDir>/src/utils/test-mocks.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
            "<rootDir>/src/**/*.test.{js,jsx}"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/jest-preprocess.js"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!(gatsby|gatsby-script)/)"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tomdewildt/gatsby-site-template"
    },
    "bugs": {
        "url": "https://github.com/tomdewildt/gatsby-site-template/issues"
    }
}