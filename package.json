{
    "name": "gatsby-site-template",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "start": "gatsby develop",
        "build": "gatsby build",
        "test": "jest test",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "lint": "eslint . --fix"
    },
    "dependencies": {
        "gatsby": "3.7.2",
        "gatsby-plugin-manifest": "3.7.1",
        "gatsby-plugin-react-helmet": "4.7.1",
        "gatsby-plugin-robots-txt": "1.6.2",
        "gatsby-plugin-sharp": "3.7.1",
        "gatsby-plugin-sitemap": "4.3.1",
        "gatsby-plugin-styled-components": "4.7.1",
        "gatsby-remark-images": "5.4.1",
        "gatsby-remark-relative-images": "2.0.2",
        "gatsby-source-filesystem": "3.7.1",
        "gatsby-transformer-remark": "4.4.1",
        "gatsby-transformer-sharp": "3.7.1",
        "prop-types": "15.7.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "styled-components": "5.3.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "11.2.7",
        "@testing-library/react-hooks": "7.0.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "27.0.2",
        "babel-preset-gatsby": "1.7.1",
        "eslint": "7.29.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-react": "7.24.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "jest": "27.0.4",
        "jest-axe": "5.0.1",
        "jest-styled-components": "7.0.4",
        "jest-watch-typeahead": "0.6.4"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "coverageReporters": [
            "lcov",
            "text"
        ],
        "setupFiles": [
            "<rootDir>/jest-loader.js"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/utils/test-matchers.js",
            "<rootDir>/src/utils/test-mocks.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
            "<rootDir>/src/**/*.test.{js,jsx}"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/jest-preprocess.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tomdewildt/gatsby-site-template"
    },
    "bugs": {
        "url": "https://github.com/tomdewildt/gatsby-site-template/issues"
    }
}
